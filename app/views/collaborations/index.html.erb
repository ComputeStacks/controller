<div class="row">
  <div class="col-lg-2 col-md-3 col-sm-12"><%= render partial: 'users/sidebar', locals: { active_tab: 'collaborations' } %></div>
  <div class="col-lg-10 col-md-9 col-sm-12">
    <div class="tab-content">
      <div class="tab-pane active">

        <div class="row">
          <div class="col-md-12">
            <%= render partial: 'collaborations/list', locals: { resource: t('obj.deployment'), collaborations: @projects } %>
          </div>
        </div>

        <div class="row">
          <div class="col-md-12">
            <%= render partial: 'collaborations/list', locals: { resource: t('obj.image'), collaborations: @images } %>
          </div>
        </div>

        <div class="row">
          <div class="col-md-12">
            <%= render partial: 'collaborations/list', locals: { resource: t('obj.registry'), collaborations: @registries } %>
          </div>
        </div>

        <div class="row">
          <div class="col-md-12">
            <%= render partial: 'collaborations/list', locals: { resource: t('obj.dns'), collaborations: @domains } %>
          </div>
        </div>

      </div>
    </div>
  </div>
</div>
