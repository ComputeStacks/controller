<% unless request.xhr? %>
  <% content_for :breadcrumb do %>
    <div class="row has-breadcrumb">
      <ol class="breadcrumb">
        <li><%= link_to t('obj.deployments'), "/admin/deployments" %></li>
        <li><%= link_to @deployment.name, admin_deployments_path(@deployment) %></li>
        <li class="active">Collaborators</li>
      </ol>
    </div>
  <% end %>
<% end %>

<%= render partial: 'collaborators/index', locals: { resource: @deployment, collaborators: @collaborators, base_path: "#{admin_deployments_path(@deployment)}/collaborators" } %>

