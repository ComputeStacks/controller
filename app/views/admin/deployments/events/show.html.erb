<% content_for :breadcrumb do %>
  <div class="row has-breadcrumb">
    <ol class="breadcrumb">
      <li><%= link_to "Projects", "/admin/deployments" %></li>
      <li><%= link_to "Project: #{@deployment.name}", @base_url %></li>
      <li><%= link_to "Events", "#{@base_url}/events" %></li>
      <li class="active">Event: <%= @log.id %></li>
    </ol>
  </div>
<% end %>

<%= render partial: "admin/event_logs/show", locals: { log: @log, subscribers: @subscribers } %>