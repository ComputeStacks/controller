<% unless request.xhr? %>
  <% content_for :title, "#{@image.label} Collaborators" %>
  <% content_for :breadcrumb do %>
    <div class="row has-breadcrumb">
      <ol class="breadcrumb">
        <li><%= link_to t('obj.images'), "/admin/container_images" %></li>
        <li><%= link_to @image.label,"/admin/container_images/#{@image.id}" %></li>
        <li class="active">Collaborators</li>
      </ol>
    </div>
  <% end %>
<% end %>

<%= render partial: 'collaborators/index', locals: { resource: @image, collaborators: @collaborators, base_path: "/admin/container_images/#{@image.id}/collaborators" } %>

