<% content_for :breadcrumb do %>
  <div class="row has-breadcrumb">
    <ol class="breadcrumb">
      <% if @volume.deployment %>
        <li><%= link_to @volume.deployment.name, "/admin/deployments/#{@volume.deployment.id}#volumes" %></li>
      <% else %>
        <li><%= link_to "Volumes", "/admin/volumes" %></li>
      <% end %>
      <li><%= link_to "Volume: #{@volume.id}", "/admin/volumes/#{@volume.id}" %></li>
      <li class="active">Event: <%= @log.id %></li>
    </ol>
  </div>
<% end %>

<%= render partial: "admin/event_logs/show", locals: { log: @log, subscribers: @subscribers } %>
