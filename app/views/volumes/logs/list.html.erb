<% unless @logs.empty? %>
  <div style="margin-top:25px;">
    <div class="panel panel-default">
      <div class="panel-body">
        <strong><%= t 'volumes.events.recent' %></strong>
      </div>
      <%= render partial: "event_logs/list", locals: { logs: @logs, root_path: "/volumes/#{@volume.id}/logs" } %>
    </div>
  </div>
<% end %>
